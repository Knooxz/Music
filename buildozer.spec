[app]

# Название приложения
title = KNPL

# Путь к исходному коду вашего приложения
package.name = com.knooxzAGP.KNPL
package.domain = org

source.dir = HZ.py

# Версия приложения
version = 1.0

# Иконка приложения
icon.filename = your_icon.png

# Список использованных библиотек и зависимостей
requirements = kivy

# Режим отладки
orientation = portrait
fullscreen = 0

# Настройка доступа к разрешениям (по необходимости)
android.permissions = INTERNET,WRITE_EXTERNAL_STORAGE

# Дополнительные параметры для Android
android.api = 27
android.ndk = 17c

# Секция для настройки версии Python
[python]

# Версия Python
version = 3.7.4